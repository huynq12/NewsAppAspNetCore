@using NewsApp.ViewModels.Seedwork;
@model ReportedList<Category,int>

@{
	Layout = null;
}


<h3 class="text-center">Top categories</h3>
<table class="table">
	<thead>
		<th>Id</th>
		<th>Category</th>
		<th>Number of posts</th>
	</thead>
	<tbody>
		@for (int i=0; i < Model.ListCount.Count; i++)
		{
			<tr>
				<td>@(i+1)</td>
				<td>@Model.Items[i].Name</td>
				<td>@Model.ListCount[i]</td>
			</tr>
			
		}
	</tbody>

</table>